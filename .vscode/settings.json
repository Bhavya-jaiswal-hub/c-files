{
  "files.associations": {
    "vector": "cpp",
    "*.tcc": "cpp",
    "functional": "cpp"
  },
  "code-runner.executorMap": {
    "cpp": "g++ -std=c++17 \"$fileName\" -o \"$fileDirname/$fileBasenameNoExtension.exe\" && \"$fileDirname/$fileBasenameNoExtension.exe\""
  },
  "code-runner.runInTerminal": true,
  "code-runner.saveFileBeforeRun": true
}
